{
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended",
    "plugin:css-modules/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["css-modules", "react", "react-hooks"],
  "parser": "babel-eslint",
  "rules": {
    "require-jsdoc": 0,
    "react-hooks/rules-of-hooks": 2,
    "react-hooks/exhaustive-deps": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
