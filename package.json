  "scripts": {
    "build": "PLATFORM=browser NODE_ENV=production webpack --config ./webpack/prod.config.js && yarn post-build",
    "dev": "PLATFORM=browser NODE_ENV=development webpack-dev-server --config ./webpack/dev.config.js --progress --color --open",
    "eslint": "eslint ./src --max-warnings 0 --cache --ext .js,.tsx,.ts",
    "post-build": "PLATFORM=node node scripts/post-build",
    "lint": "yarn eslint && yarn stylelint",
    "serve": "serve build/ -l 9000",
    "stylelint": "stylelint 'src/**/*.css' --cache",
    "tsc": "tsc",
    "wba": "WBA=1 yarn build"
  },
